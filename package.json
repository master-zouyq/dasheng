{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@uiw/react-code-preview": "^1.11.13",
    "@umijs/preset-react": "^1.7.6",
    "@umijs/test": "^3.2.4",
    "fingerprintjs2": "^2.1.2",
    "gsap": "^3.7.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-gsap": "^3.2.1",
    "react-markdown": "^6.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.3",
    "redux-form": "^8.3.7",
    "remark-gfm": "^1.0.0",
    "umi": "^3.3.0",
    "yorkie": "^2.0.0"
  }
}
